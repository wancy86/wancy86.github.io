export=1
body=IF @@version LIKE '%SQL Server 2005%'OR @@version LIKE '%SQL Server 2008%'OR @@version LIKE '%SQL Server 2012%'OR @@version LIKE '%SQL Azure%'	SELECT DISTINCT 		p.state_desc COLLATE DATABASE_DEFAULT + ' ' + p.permission_name  COLLATE DATABASE_DEFAULT 			+ ' ON [' + :REFACTORING_SCHEMA + '].[' + :REFACTORING_OLD_NAME 			+ '] TO [' + u.name COLLATE DATABASE_DEFAULT + ']'	FROM	sys.database_permissions p,		sys.schemas s,		sys.database_principals u,		sys.objects o	WHERE	o.name = :REFACTORING_OLD_NAME		AND s.schema_id = o.schema_id		AND s.name = :REFACTORING_SCHEMA		AND u.principal_id = p.grantee_principal_id		AND p.major_id = o.object_id		AND p.class = 1ELSEBEGIN 	DECLARE @SQL NVARCHAR(2000)	SELECT @SQL =	'SELECT DISTINCT 		upper(v2.name COLLATE DATABASE_DEFAULT) + '' '' + upper(v1.name COLLATE DATABASE_DEFAULT) 			+ '' ON [''' + :REFACTORING_SCHEMA + '''].[''' + :REFACTORING_OLD_NAME  			+ '''] TO ['' + user_name(p.uid) COLLATE DATABASE_DEFAULT + '']''	FROM	dbo.sysprotects p,		master.dbo.spt_values v1,		master.dbo.spt_values v2,		master.dbo.spt_values v3,		dbo.sysobjects o	WHERE	v1.type = ''T''		AND v1.number = p.action		AND v2.type = ''T''		AND v2.number = p.protecttype		AND o.xtype = substring(v3.name,1,2) collate database_default 		AND v3.type = ''O9T''		AND p.id != 0		AND o.id = p.id		AND o.name = ''' + :REFACTORING_OLD_NAME + '''		AND user_name(o.uid) = ''' + :REFACTORING_SCHEMA + ''''	 	EXEC (@SQL)END

[rfRules.1.11]
